  "devDependencies": {
    "@phanect/eslint-plugin": "latest",
    "@types/debug": "^4.1.5",
    "@types/express": "^4.17.1",
    "@types/jest": "^25.1.2",
    "@types/node": "^12.0.2",
    "@types/puppeteer": "^2.0.0",
